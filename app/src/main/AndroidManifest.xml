<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.oneforall">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.OneForAll"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".activity.ComposeTestActivity"
            android:exported="true"
            android:label="@string/title_activity_compose_test"
            android:theme="@style/Theme.OneForAll.NoActionBar" />
        <activity
            android:name=".activity.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.ProxyActivity" />
        <activity android:name=".activity.RetrofitActivity" />
        <activity android:name=".activity.OkhttpActivity" />
        <activity android:name=".activity.RxJavaActivity" />
        <activity android:name=".activity.ServiceActivity" />
        <activity android:name=".activity.KotlinActivity" />
        <activity android:name=".activity.ThreadActivity" />

        <activity android:name=".activity.PackageManagerActivity" />

        <service android:name=".service.StartService" />
        <service android:name=".service.MusicPlayerService"/>
    </application>

    <uses-permission android:name="android.permission.INTERNET" />

</manifest>